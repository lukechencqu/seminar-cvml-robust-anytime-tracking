\DeclareOptionBeamer{noframetitlebg}{\beamer@frametitlebgfalse}
\ProcessOptionsBeamer

\useoutertheme{fawkessimple}
\useinnertheme{fawkessimple}
\usecolortheme{fawkessimple}

%\setbeamertemplate{footline}[infolines theme]
